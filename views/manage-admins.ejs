<%- include('layouts/header.ejs') %>
<%- include('layouts/admin-nav.ejs') %>
<div id="page_content">
    <div id="page_content_inner">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                <div class="uk-overflow-container">
                    <table class="uk-table uk-table-nowrap table_check">
                        <thead>
                        <tr>
                           
                          
                            <th class="uk-width-2-10">Name</th>
                            <th class="uk-width-2-10 ">Phone</th>
                            <th class="uk-width-2-10 ">Status</th>
                            <th class="uk-width-2-10 ">Actions</th>
                        </tr>
                        </thead>
                        <tbody>
                            <% for (var i = 0; i < admins.length; i++) { %>
                               
                                <tr>
                                    <td><%= admins[i].name %></td>
                                    <td ><%= admins[i].phone %></td>
                                    <td ><%= admins[i].status %></td>
                                    <td class="">
                                        <a href="/edit-admin/<%= admins[i].user_id %>"><i class="md-icon material-icons">&#xE254;</i></a>
                                        <a href="/deactivate-admin/<%= admins[i].user_id %>"><i class="md-icon material-icons">&#xE88F;</i></a>
                                        <a href="/activate-admin/<%= admins[i].user_id %>"><i class="md-icon material-icons">&#xE88F;</i></a>
                                    </td>
                                </tr>
                              <% } %>
                            
                           
                        </tbody>
                    </table>
                </div>
                
            </div>
        </div>
    </div>
</div>
<script src="custom-js/admin-add.js"></script>
<%- include('layouts/footer.ejs') %>